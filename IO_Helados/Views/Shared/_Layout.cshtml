<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title style="text-align:center" >@ViewBag.Title - Proyecto IO</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="background-image: url(https://i.ibb.co/wBVjqtL/Helado.jpg);">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">               
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Proyecto_IO", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div style="padding-bottom:0px" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Helados", "index", "Helados")</li>
                    <li>@Html.ActionLink("Ingredientes", "index", "Ingredientes")</li>
                    <li>@Html.ActionLink("Helados e Ingredientes", "index", "Hel_Ing")</li>
                    <li>@Html.ActionLink("Ingredientes y Unidades disponibles", "index", "Ing_Und")</li>
                    <li>@Html.ActionLink("Helado y Utilidad", "index", "Hel_Uti")</li>
                    <li>@Html.ActionLink("Nro de restricciones de demanda", "index", "Nro_Restr")</li>
                    <li>@Html.ActionLink("Helado y Demanda", "index", "Hel_Dem")</li>
                    <li>@Html.ActionLink("Demanda y Helados", "index", "Dem_Hel")</li>
                    <li>@Html.ActionLink("Helados y Cantidad", "index", "Hel_Cant")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Proyecto IO</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
